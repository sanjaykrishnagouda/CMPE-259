# ga-real-vrand

ga-real-vrand-s1.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 1

ga-real-vrand-s2.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 2

ga-real-vrand-s3.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 3

ga-real-vrand-s4.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 4

ga-real-vrand-s5.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 5

ga-real-vrand-s6.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 6

ga-real-vrand-s7.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 7

ga-real-vrand-s8.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 8

ga-real-vrand-s9.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 9

ga-real-vrand-s10.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -G 3 -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 600 -s 10


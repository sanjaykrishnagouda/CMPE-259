	./visibility -hst $(DISH) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -S -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 100000 -T 100 -M 150 -l 0.7 -d 35 -Sd -er 35 -cr 170 -s 1

dsa-dish-cr-170-s-2.bmp : visibility
	./visibility -hst $(DISH) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -S -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 100000 -T 100 -M 150 -l 0.7 -d 35 -Sd -er 35 -cr 170 -s 2

dsa-dish-cr-170-s-3.bmp : visibility
	./visibility -hst $(DISH) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -S -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 100000 -T 100 -M 150 -l 0.7 -d 35 -Sd -er 35 -cr 170 -s 3


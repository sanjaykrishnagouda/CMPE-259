# ==============

# sa-real

sa-real-s-1.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -S -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 100000 -T 100 -M 500 -l 0.7 -d 50 -s 1

sa-real-s-2.bmp : visibility
	./visibility -hst $(REAL) -hst-latlon 37 -123 -w $(WIN_240x180A) -r 50 -S -bmp $@@ -txt $(basename $@@).txt -vrand 10 -t 100000 -T 100 -M 500 -l 0.7 -d 50 -s 2

sa-real-s-3.bmp : visibility

#	./visibility -h ../Data/SRTM_1_Arc_Second/N37W123.hgt -123 37 -w -122.2 -122.1 37.1 37.2 -r 150 -C $@@
#
a57 4
# The -m filenames can't contain dashes because MATLAB interprets
# dash characters as minus operators.
braemoor_height_2.m : braemoor-height-2.bmp
	./bmp2matlab braemoor-height-2.bmp -m z -o braemoor_height_2.m
d59 2
a60 2
braemoor_limited_25_cvis_2.m : braemoor-limited-25-cvis-2.bmp
	./bmp2matlab braemoor-limited-25-cvis-2.bmp -m c -o braemoor_limited_25_cvis_2.m
a61 2
braemoor_limited_50_cvis_2.m : braemoor-limited-50-cvis-2.bmp
	./bmp2matlab braemoor-limited-50-cvis-2.bmp -m c -o braemoor_limited_50_cvis_2.m
d63 2
a64 2
braemoor_limited_100_cvis_2.m : braemoor-limited-100-cvis-2.bmp
	./bmp2matlab braemoor-limited-100-cvis-2.bmp -m c -o braemoor_limited_100_cvis_2.m
d67 2
a68 2
braemoor_limited_150_cvis_2.m : braemoor-limited-150-cvis-2.bmp
	./bmp2matlab braemoor-limited-150-cvis-2.bmp -m c -o braemoor_limited_150_cvis_2.m
@


